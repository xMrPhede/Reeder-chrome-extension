{
    "manifest_version": 3,
    "name": "Share to Reeder",
    "version": "1.0",
    "description": "Sends the current tab's URL to the macOS Reeder app via native bridge.",
    "permissions": ["nativeMessaging", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Share to Reeder"
    },
    "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  }
  }
  